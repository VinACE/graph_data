{
  "applications": [
    {
      "name": "InventorySystem",
      "functions": [
        {"name": "addItem", "variables": ["itemName", "quantity", "price"]},
        {"name": "removeItem", "variables": ["itemName", "quantity"]},
        {"name": "updateStock", "variables": ["itemName", "newQuantity"]}
      ]
    },
    {
      "name": "EcommercePlatform",
      "functions": [
        {"name": "processOrder", "variables": ["orderId", "paymentMethod"]},
        {"name": "cancelOrder", "variables": ["orderId", "reason"]},
        {"name": "refundOrder", "variables": ["orderId", "amount"]}
      ]
    },
    {
