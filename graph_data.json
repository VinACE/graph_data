      "functions": [
        {"name": "hireEmployee", "variables": ["employeeName", "position"]},
        {"name": "terminateEmployee", "variables": ["employeeId", "reason"]},
        {"name": "approveLeave", "variables": ["employeeId", "days"]}
      ]
    },
    {
      "name": "BankingApp",
      "functions": [
        {"name": "transferFunds", "variables": ["fromAccount", "toAccount", "amount"]},
        {"name": "openAccount", "variables": ["customerName", "accountType"]},
        {"name": "closeAccount", "variables": ["accountNumber"]}
      ]
    },
    {
      "name": "HealthcareSystem",
      "functions": [
        {"name": "bookAppointment", "variables": ["patientId", "doctorId", "time"]},
        {"name": "issuePrescription", "variables": ["patientId", "medications"]},
        {"name": "updateMedicalRecord", "variables": ["patientId", "record"]}
      ]
    },
    {
      "name": "LearningPlatform",
      "functions": [
        {"name": "enrollCourse", "variables": ["studentId", "courseId"]},
