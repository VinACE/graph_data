      "functions": [
        {"name": "postStatus", "variables": ["userId", "content"]},
        {"name": "likePost", "variables": ["userId", "postId"]},
        {"name": "addFriend", "variables": ["userId", "friendId"]}
      ]
    },
    {
      "name": "AnalyticsDashboard",
      "functions": [
        {"name": "generateReport", "variables": ["reportType", "dateRange"]},
        {"name": "trackMetrics", "variables": ["metricName", "dateRange"]},
        {"name": "visualizeTrends", "variables": ["metricName", "timePeriod"]}
      ]
